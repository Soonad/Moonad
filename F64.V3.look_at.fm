F64.V3.look_at: F64.V3 -> F64.V3 -> F64.V3 -> F64.V3 
  (a) (b) (c)
  a<() F64.V3> | (a.x) (a.y) (a.z)
  b<() F64.V3> | (a.x) (a.y) (a.z)
  c<() F64.V3> | (a.x) (a.y) (a.z)
  let a_eql_b  = F64.V3.eql(a)(b)
  let diff     = F64.V3.sub(b)(a)
  let normdiff = F64.V3.norm(diff)
  a_eql_b<() F64.V3> 
  | normdiff ;
  | c ;;;;
   
