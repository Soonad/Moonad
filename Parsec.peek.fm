// we can look ahead in the input stream and not consume any input
// if the parser succeeds

Parsec.peek: <A: Type> -> Parsec(A) -> Parsec(A)
  <A> (p)
  <X> (s1) (empty_value) (empty_error) (eaten_value) (eaten_error)
  p<X>(s1)
  | (s2) (a) empty_value(s1)(a);
  | empty_error;
  | (s2) (a) empty_value(s1)(a);
  | eaten_error;
