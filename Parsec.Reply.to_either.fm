// We can collapse a parser reply to an Either by throwing away consumption info

Parsec.Reply.to_either: <A: Type> -> Parsec.Reply(A) -> Either(Parsec.Error)(A)

  <A> (reply)
  reply<() Either(Parsec.Error)(A)>
  | () (a) Either.right<Parsec.Error><A>(a);
  | () (e) Either.left<Parsec.Error><A>(e);
  | () (a) Either.right<Parsec.Error><A>(a);
  | () (e) Either.left<Parsec.Error><A>(e);

