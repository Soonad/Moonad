TA.Thing.cast
  : TA.Thing     ->
    F64               ->
    List(TA.Game.Hit) ->  
    TA.Thing
  (self) (at_tik) (do_hit)
  let curr_tik = TA.Thing.get_tik(self)
  F64.eql(curr_tik)(at_tik)<() TA.Thing>
  | TA.Thing.set_hit(self)(do_hit);
  | self;
  
