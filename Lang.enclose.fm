Lang.enclose: <A: Type> -> Parsec(A) -> Parsec(Pair(Bool)(A))
  <A> (p)
  Parsec.bind<Bool><Pair(Bool)(A)>(Lang.open) | (eras)
  Parsec.bind<A><Pair(Bool)(A)>(p) | (x)
  Parsec.then<String><Pair(Bool)(A)>
  | eras<() Parsec(String)>(Parsec.tokens(">"))(Parsec.tokens(")"));
  | Parsec.pure<Pair(Bool)(A)>(Pair.new<Bool><A>(eras)(x));;;
