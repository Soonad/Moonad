Parsec.many_till: <A: Type> -> <B: Type> -> Parsec(A) -> Parsec(B) -> Parsec(List(A))
  <A> <B> (p) (q)
  Parsec.plus<List(A)>
  | Parsec.then<B><List(A)>(q)(Parsec.pure<List(A)>(List.nil<A>));
  | Parsec.bind<A><List(A)>(p) | (x)
    Parsec.bind<List(A)><List(A)>(Parsec.many_till<A><B>(p)(q))
    | (xs) Parsec.pure<List(A)>(List.cons<A>(x)(xs));;;

