// A dependent type of parser reply for use by Parsec.parse

Parsec.type :
  (S: Type) ->
  (E: Type) ->
  (A: Type) ->
  (E -> String) ->
  Parsec.Reply(S)(E)(A) ->
  Type

  (S) (E) (A) (E.show) (reply) 
  reply<() Type>
  | () () A;
  | () (e) The(Debug)(Debug.display(Parsec.Error.show<E>(E.show)(e)));
  | () () A;
  | () (e) The(Debug)(Debug.display(Parsec.Error.show<E>(E.show)(e)));
