Lang.line_comment: Parsec(Unit)
  let not_newline = ((c) Bool.not(U16.eql(c)(Char.newline))) :: Char -> Bool
  Parsec.then<String><Unit>(Parsec.tokens("/")) |
  Parsec.then<String><Unit>(Parsec.take_while(not_newline)) |
  Parsec.then<Char  ><Unit>(Parsec.char(Char.newline)) |
  Parsec.pure<Unit>(Unit.new);;;
