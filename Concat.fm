// A concatenation list.
Concat: (A: Type) -> Type
  (A) List(A) -> List(A)

Concat.empty<A: Type>: Concat(A)
  (xs) xs

Concat.push<A: Type>(val: A, con: Concat(A)): Concat(A)
  (xs) con(List.cons<>(val, xs))

//Converts a list to a concatenation list.
Concat.to_list<A: Type>(xs: Concat(A)): List(A)
  xs(List.nil<A>)

