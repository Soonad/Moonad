// We can observe an error so that we can try to handle it
Parsec.observing: <A: Type> -> Parsec(A) -> Parsec(Either(Parsec.Error)(A))

  <A> (p)
  <X> (s1) (empty_value) (empty_error) (eaten_value) (eaten_error)
  p<X>(s1)
  | (s2) (x) empty_value(s2)(Either.right<Parsec.Error><A>(x));
  | (s2) (e) empty_value(s2)(Either.left<Parsec.Error><A>(e));
  | (s2) (x) eaten_value(s2)(Either.right<Parsec.Error><A>(x));
  | (s2) (e) eaten_value(s2)(Either.left<Parsec.Error><A>(e));
